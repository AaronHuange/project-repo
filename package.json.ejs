{
  "name": "<%- projectName %>",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "license": "UNLICENSED",
  "author": "",
  "scripts": {
    "build": "nest build && tsc-alias",
    "format": "prettier --write 'src/**/*.{ts,json,yml,yaml}'",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migration:gen": "typeorm migration:create",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
    "@apollo/gateway": "^2.4.7",
    "@apollo/server": "^4.7.3",
    "@as-integrations/fastify": "^2.0.0",
    "@fastify/compress": "6.4.0",
    "@fastify/helmet": "10.1.1",
    "@fastify/rate-limit": "8.0.0",
    "@nestjs/apollo": "^11.0.6",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.4.3",
    "@nestjs/core": "^9.4.3",
    <%_ if (TYPE === 'GraphQL') { -%>
    "@nestjs/graphql": "^11.0.6",
    "graphql": "^16.6.0",
    <%_ } -%>
    "@nestjs/platform-fastify": "^9.4.3",
    "@nestjs/typeorm": "^9.0.1",
    <%_ if (monitor) { -%>
    "nest-status-monitor": "^0.1.4",
    "@nestjs/platform-socket.io": "7.0.0",
    "@nestjs/websockets": "^10.0.5",
    <%_ } -%>
    "axios": "^1.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.1.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "fastify": "^4.17.0",
    "lodash": "^4.17.21",
    "lodash.camelcase": "^4.3.0",
    "lodash.snakecase": "^4.1.1",
    "module-alias": "^2.2.3",
    "mysql2": "^3.3.5",
    "reflect-metadata": "0.1.13",
    "rimraf": "4.4.1",
    "rxjs": "7.8.1",
    "sqlutils": "^1.5.3",
    "ts-morph": "^18.0.0",
    "tsc-alias": "^1.8.6",
    "typeorm": "^0.3.16",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.5.0",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^9.4.3",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.1",
    "@types/node": "18.16.3",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.59.1",
    "@typescript-eslint/parser": "5.59.1",
    "eslint": "8.39.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.5.0",
    "prettier": "2.8.8",
    "supertest": "6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "4.9.5"
  }
}
